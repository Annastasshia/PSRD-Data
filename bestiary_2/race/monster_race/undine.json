    "sections": [
        {
            "body": "<p id=\"+2-dexterity,-+2-wisdom,--2-strength\">Undines are both perceptive and agile, but tend to adapt rather than match force with force. </p>", 
            "source": "Bestiary 2", 
            "type": "section", 
            "name": "+2 Dexterity, +2 Wisdom, -2 Strength", 
            "section_id": 2083
        }, 
        {
            "body": "<p id=\"speed\">Undines have a swim speed of 30 feet.</p>", 
            "name": "Speed", 
            "url": "pfsrd://Bestiary 2/Monsters/Undine/Undine/Speed", 
            "section_id": 2084, 
            "source": "Bestiary 2", 
            "type": "section"
        }, 
        {
            "body": "<p id=\"darkvision\">Undines can see in the dark up to 60 feet.</p>", 
            "source": "Bestiary 2", 
            "type": "section", 
            "name": "Darkvision", 
            "section_id": 2085
        }, 
        {
            "body": "<p id=\"spell-like-abilities\"><i>Hydraulic push</i> 1/day. (Caster level equals the undine's total Hit Dice.)</p>", 
            "source": "Bestiary 2", 
            "type": "section", 
            "name": "Spell-Like Abilities", 
            "section_id": 2086
        }, 
        {
            "body": "<p id=\"elemental-resistance\">Undines have cold resistance 5.</p>", 
            "name": "Energy Resistance", 
